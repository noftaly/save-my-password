{
  "name": "save-my-password",
  "version": "0.1.0",
  "description": "A simple yet secured free password manager",
  "main": "app.js",
  "private": false,
  "scripts": {
    "start": "concurrently \"npm run watch:js\" \"npm run watch:sass\"",
    "watch:js": "nodemon --exec babel-node app.js",
    "watch:sass": "node-sass -w sass/ -o public/css/ -r",
    "build:sass": "node-sass sass/ -o public/css/ -r",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Elliot 'noftaly' Maisl <elliot.maisl@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-hbs": "^2.3.1",
    "express-session": "^1.17.0",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^3.21.3",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-noftalint": "^1.3.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-unicorn": "^17.2.0",
    "nodemon": "^2.0.2"
  }
}
